	kf4=\E[20~, kf5=\E[21~, kf6=\E[23~, kf7=\E[24~,
	kf8=\E[25~, khome=\E[1~, kich1=\E[2~, kll=\E[4~,
	knp=\E[6~, kpp=\E[5~, nel=\EE, rc=\E8, rev=\E[7m,
	rf=/usr/share/tabset/vt100, ri=\EM, rin=\E[%p1%dT,
	rmacs=\E(B, rmam=\E[?7l, rmir=\E[4l, rmkx=\E[?1l\E>,
