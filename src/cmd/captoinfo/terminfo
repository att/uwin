	dch1=\E#$<2.1*>, dl1=\EV$<49.6>, ed=^_$<2*>,
	el=\EL$<2.1>, ht=\t, il1=\EU$<46>, ind=\n, is2=\E\,,
	kbs=\b, kcub1=\b, kcud1=\n, kcuu1=^K, nel=\r\n,
