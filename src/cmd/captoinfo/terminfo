	kri=\E\\, krmir=\E\200, mc4=^^o\s\E\EQ!\EYP^W,
	mc5=\EQ"\EY(^W\EYD\Eo\s^^, prot=\EI,
	rep=\Er%p1%c%p2%'\s'%+%c, rev=\ED,
	rmcup=\Ev\s\s\s\s\Ep\r\n, rmir=\E\s\s, rmkx=\Ex,
	rmso=\Ed, rmul=\Eg, sgr0=\EN@,
