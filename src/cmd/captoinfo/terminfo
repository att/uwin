	flash=\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERBA4\ERB0,
	home=\ELF7l^?\s@, ht=\t, ind=\n,
	is2=\ELZ\EKA0\ELF7l^?\s@, ll=\ELF\shl\s@, rmso=\EMT1,
