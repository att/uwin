	cuu=\E[%p1%dA, cuu1=\EM, dch=\E[%p1%dP, dch1=\E[P,
	dl=\E[M, ed=\E[J\E[r, el=\E[K\E[t, flash=\E$\E$P,
	home=\E$B, ht=\t, il1=\E[L\E[t, ind=\n, kbs=\b,
	kcub1=\E[D, kcud1=\E[B, kcuf1=\E[C, kcuu1=\E[A,
