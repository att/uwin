	cuf1=\f, cup=\E=%p1%'\s'%+%c%p2%'\s'%+%c$<3>, cuu1=^K,
	cvvis=\E.2, dch1=\EW, dl1=\ER$<1*>, dsl=\Ef\r\Eg,
	ed=\EY, el=\ET, flash=\Eb\Ed, fsl=\Eg, home=^^, ht=\t,
	ich1=\EQ, if=/usr/lib/tabset/stdcrt, il1=\EE, ind=\n,
	is2=\El\E"\EF1\E.3^O\EA\E<, kbs=\b, kclr=^Z, kcub1=\b,
	kcud1=^V, kcuf1=\f, kcuu1=^K, kdch1=\EW,
	kdl1=\ER$<1*>, ked=\EY, kel=\ET, kich1=\EQ, kil1=\EE,
