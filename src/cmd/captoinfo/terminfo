	kf9=\EOw, rc=\E8, rev=\E[7m, ri=\EM, rmacs=^O,
	rmam=\E[?7l, rmkx=\E[?1l\E>, rmso=\E[m, rmul=\E[m,
	rs2=\E>\E[?3l\E[?4l\E[?5l\E[?7h\E[?8h, sc=\E7,
	sgr0=\E[m^O, smacs=^N, smam=\E[?7h, smkx=\E[?1h\E=,
	smso=\E[1;7m, smul=\E[4m, tbc=\E[3g,
