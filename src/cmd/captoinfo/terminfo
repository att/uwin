	eslok, hs, xon,
	cols#80, it#8, lines#24,
	bel=^G, blink=\E[5m, bold=\E[2m, clear=\E[2J, cr=\r,
	csr=\E[%i%p1%d;%p2%dr, cub=\E[%p1%dD, cub1=\b,
	cud=\E[%p1%dB, cud1=\n, cuf=\E[%p1%dC, cuf1=\E[C,
	cup=\E[%i%p1%d;%p2%dH, cuu=\E[%p1%dA, cuu1=\E[A,
	dch=\E[%p1%dP, dch1=\E[1P, dim=\E[2m, dl=\E[%p1%dM,
	dsl=\E[>1l, ed=\E[J, el=\E[K, fsl=\E[u\E[>5l,
	home=\E[H, ht=\t, hts=\EH,
	if=/usr/share/tabset/zenith29, il=\E[%p1%dL, ind=\ED,
	kbs=\b, kclr=\E[J, kcub1=\EOD, kcud1=\EOB, kcuf1=\EOC,
	kcuu1=\EOA, ked=\E[J, kf0=\E[~, kf1=\EOS, kf2=\EOT,
	kf3=\EOU, kf4=\EOV, kf5=\EOW, kf6=\EOP, kf7=\EOQ,
	kf8=\EOR, kf9=\EOX, khome=\E[H, lf0=help, mc0=\E#7,
	nel=\r\ED, rc=\E[r, rev=\E[7m, ri=\EM, rmcup=\E[?7h,
	rmso=\E[m, rmul=\E[m,
	rs1=\E<\E[1;24r\E[24;1H\E[?7h\E[>4h\E[>1;2;3;5;6;7;8;9l\E[m\E[11m,
	sc=\E[s, sgr0=\E[m, smcup=\E[?7l, smso=\E[7;2m,
	smul=\E[4m, tbc=\E[3g,
	tsl=\E[s\E[>5;1h\E[25;%i%p1%dH\E[1K,
