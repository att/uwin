	ed=\E[J, el=\E[K, flash=\E[?5h\E[?5l, fsl=\E8,
	home=\E[H, ht=\t, hts=\EH, ich=\E[%p1%d@,
	il=\E[%p1%dL, il1=\E[L, ind=\n, indn=\E[%p1%dE,
	is1=\E<\E[?3l,
	is2=\E[m^O\E[1;2;3;4;6l\E[12;13;14;20l\E[?6;97;99l\E[?7h\E[4i\Ex\E[25;1j\212,
