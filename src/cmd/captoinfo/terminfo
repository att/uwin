	use=scrhp,

hp2621-a|hp2621a-a|2621-a|hp2621 with fn as arrows,
	use=hp2621-fl,

# newer hewlett packard terminals

newhpkeyboard|generic entry for HP extended keyboard,
	kbs=\b, kcbt=\Ei, kclr=\EJ, kcub1=\ED, kcud1=\EB,
	kcuf1=\EC, kcuu1=\EA, kdch1=\EP, kdl1=\EM, ked=\EJ,
	kel=\EK, khome=\Eh, kich1=\EQ, kil1=\EL, kind=\ET,
	kll=\EF, knp=\EU, kpp=\EV, kri=\ES, krmir=\ER,
	rmkx=\E&s0A, smkx=\E&s1A,
	use=hp+pfk-cr,

newhp|generic entry for new hewlett packard terminals,
	am, bw, mir, xhp,
	cols#80, lines#24, pb#4800,
	acsc=T1R!U2S"W3O#V4P$t5u6w7v8:'9(LQKWlRkT5I3@2[MAJSmFjGdHQ;Y+Z*X:4>q\\\,x.n/,
	blink=\E&dA, bold=\E&dF, cbt=\Ei, cub1=\b, cuf1=\EC,
	cuu1=\EA, dch1=\EP, dim=\E&dH, dl1=\EM, ed=\EJ,
	el=\EK, ht=\t, hts=\E1, il1=\EL, invis=\E&dS,
	is1=\E&jB, nel=\r\n, rev=\E&dB, ri=\ET, rmacs=^O,
	rmir=\ER, rmso=\E&d@, rmul=\E&d@, rs1=\Eg,
	sgr0=\E&d@^O, smacs=^N, smir=\EQ, smso=\E&dJ,
	smul=\E&dD, tbc=\E3,
	use=newhpkeyboard,

memhp|memory relative addressing for new HP ttys,
	vt#6,
	clear=\EH\EJ, cub=\E&a-%p1%dC, cud=\E&a+%p1%dR,
	cuf=\E&a+%p1%dC, cup=\E&a%p1%dr%p2%dC,
	cuu=\E&a-%p1%dR, home=\EH, hpa=\E&a%p1%dC,
	ll=\E&a23R\r, mrcup=\E&a%p1%p1%dr%p2%p2%dC,
	vpa=\E&a%p1%dR,
	use=newhp,

scrhp|screen relative addressing for new HP ttys,
	clear=\E&a0c0Y\EJ, cub=\E&a-%p1%dC, cud=\E&a+%p1%dR,
	cuf=\E&a+%p1%dC, cup=\E&a%p1%dy%p2%dC,
	cuu=\E&a-%p1%dR, home=\E&a0y0C, hpa=\E&a%p1%dC,
	ll=\E&a0y0C\EA, mrcup=\E&a%p1%p1%dr%p2%p2%dC,
	vpa=\E&a%p1%dY,
	use=newhp,

hp+labels|"standard" label info for new HP ttys,
	xon,
	lh#2, lw#8, nlab#8,
	bel=^G, cr=\r, cud1=\n, ind=\n, smln=\E&jB,

