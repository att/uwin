	dch1=\EW, dl1=\ER, ed=\Ey, el=\Et,
	flash=\EU\EX\EU\EX\EU\EX\EU\EX, fsl=\E.2, home=^^,
	ht=\t, ich1=\EQ, if=/usr/share/tabset/std, il1=\EE,
	ind=\n, is2=\EC\Eu\E'\E(\El\EA\E%\E{\E.2\EG0\Ed\En,
	kcub1=\b, kcud1=^V, kcuf1=\f, kcuu1=^K, kf0=^A0\r,
