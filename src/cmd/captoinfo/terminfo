	cuu1=\EA, cvvis=\Ex4, dch1=\EN, dl1=\EM, ed=\EJ,
	el=\EK, fsl=\Ek\Ey5, home=\EH, ht=\t, il1=\EL, ind=\n,
	ip=<1.5/>$<1.5>, kbs=\b, kcub1=\ED, kcud1=\EB,
	kcuf1=\EC, kcuu1=\EA, kf1=\ES, kf2=\ET, kf3=\EU,
	kf4=\EV, kf5=\EW, kf6=\EP, kf7=\EQ, kf8=\ER,
	khome=\EH, lf6=blue, lf7=red, lf8=white, ri=\EI,
	rmacs=\EG, rmir=\EO, rmso=\Eq, smacs=\EF, smir=\E@,
	smso=\Ep, tsl=\Ej\Ex5\EY8%p1%{32}%p1%'%'%+%cc\Eo\Eo,
h19-u|heathkit with underscore cursor,
	cnorm@, cvvis@,
	use=h19-b,
h19-us|h19us|heathkit w/keypad shifted/underscore cursor,
	rmkx=\Eu, smkx=\Et,
	use=h19-u,
