	cup=\EY%p1%'\s'%+%c%p2%'\s'%+%c, cuu1=\EA, cvvis=\Ey2,
	dch1=\Ef, dl1=\EF, ed=\EJ, el=\EK, home=\EH, ht=\t,
	ich1=\Ee, il1=\EE, ind=\n, kcub1=\377\244,
